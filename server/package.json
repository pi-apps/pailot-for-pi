{
   "name": "pailot",
   "version": "1.0.0",
   "main": "index.js",
   "description": "Pailot is a decentralized delivery service application that is focused on piloting pioneers into full decentralized logistics, food, healthcare, etc.",
   "private": "true",
   "scripts": {
      "prebuild": "rimraf build",
      "build": "tsc --project ./tsconfig.build.json",
      "build:watch": "tsc --watch --project ./tsconfig.build.json",
      "prestart": "yarn build",
      "start": "ts-node src/index.ts",
      "dev": "concurrently --raw -n \"tsc,server\" -s \"last\" -c \"magenta,yellow\" \"yarn build:watch\" \"yarn start\"",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "format:write": "prettier --write \"src/**/*.ts\"",
      "format:check": "prettier --check \"src/**/*.ts\"",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "dependencies": {
      "axios": "^1.2.3",
      "cloudinary": "^1.33.0",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "morgan": "^1.10.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.11"
   },
   "devDependencies": {
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.15",
      "@types/morgan": "^1.9.4",
      "@types/node": "^16.11.10",
      "@typescript-eslint/eslint-plugin": "^5.48.2",
      "@typescript-eslint/parser": "^5.48.2",
      "concurrently": "^7.6.0",
      "eslint": "^8.32.0",
      "eslint-config-prettier": "^8.6.0",
      "eslint-plugin-prettier": "^4.2.1",
      "nodemon": "^2.0.20",
      "prettier": "^2.8.3",
      "rimraf": "^4.1.1",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   }
}
